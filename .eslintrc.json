{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "indent": ["error", "tab"],
    "no-console": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"]
  }
}
